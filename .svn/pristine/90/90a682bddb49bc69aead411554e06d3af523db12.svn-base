package com.example.myimageapp.model;

import java.io.Serializable;

public class MovieImageModel implements Serializable {
    private int id;
    private String name;
    private String url;

    public MovieImageModel(int id, String name, String url) {
        this.id = id;
        this.name = name;
        this.url = url;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getUrl() {
        return url;
    }

    public void setUrl(String url) {
        this.url = url;
    }
}
